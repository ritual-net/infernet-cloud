{
	"name": "infernet-cloud",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:debug": "NODE_OPTIONS='--inspect=localhost:9229' vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"edgedb:init": "edgedb project init; npm run edgedb:generate",
		"edgedb:migrate": "edgedb migration create; edgedb migrate; npm run edgedb:generate",
		"edgedb:generate": "npx @edgedb/generate interfaces && npx @edgedb/generate edgeql-js --target ts"
	},
	"devDependencies": {
		"@edgedb/generate": "^0.4.1",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/node": "^20.11.5",
		"@types/tar": "^6.1.10",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tar": "^6.2.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-ec2": "^3.507.0",
		"@google-cloud/compute": "^4.1.0",
		"axios": "^1.6.5",
		"edgedb": "^1.4.1",
		"googleapis": "^131.0.0"
	}
}
