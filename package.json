{
	"name": "infernet-cloud",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:docker": "docker compose build --force-rm --no-cache && docker compose up --detach && docker compose logs -f",
		"dev:debug": "NODE_OPTIONS='--inspect=localhost:9229' vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"edgedb:init": "edgedb project init; npm run edgedb:migrate",
		"edgedb:migrate": "edgedb migration create; edgedb migrate; npm run edgedb:generate",
		"edgedb:generate": "npx @edgedb/generate interfaces && npx @edgedb/generate edgeql-js --target ts",
		"edgedb:destroy": "edgedb instance destroy -I infernet_cloud --force",
		"edgedb:cli": "edgedb"
	},
	"devDependencies": {
		"@edgedb/generate": "^0.4.1",
		"@melt-ui/pp": "^0.3.0",
		"@melt-ui/svelte": "^0.70.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/node": "^20.11.5",
		"@types/tar": "^6.1.10",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"postcss-nesting": "^12.1.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tar": "^6.2.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.0.0"
	},
	"dependencies": {
		"@aws-sdk/client-ec2": "^3.507.0",
		"@fontsource/barlow": "^5.0.8",
		"@google-cloud/compute": "^4.1.0",
		"@tanstack/svelte-query": "^5.28.6",
		"aws-sdk": "^2.1540.0",
		"axios": "^1.6.5",
		"dotenv": "^16.4.3",
		"edgedb": "^1.4.1",
		"googleapis": "^131.0.0",
		"openai": "^4.30.0",
		"svelte-headless-table": "^0.18.1",
		"sveltekit-flash-message": "^2.4.4",
		"sveltekit-superforms": "^2.10.6",
		"yup": "^1.4.0"
	},
	"type": "module"
}
